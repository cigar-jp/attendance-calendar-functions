  "dependencies": {
    "@google-cloud/functions-framework": "^3.0.0",
    "@google-cloud/storage": "^7.0.0",
    "csv-parse": "^5.0.0",
    "dotenv": "^16.0.0",
    "googleapis": "^120.0.0",
    "iconv-lite": "^0.6.0",
    "jszip": "^3.10.0",
    "tslib": "^2.5.0"
